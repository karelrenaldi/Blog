<div class="create-form">
    <h1>Leaderboard Form</h1>
    <div class="row p-4">
        <div class="col-md-10 form">
            <form action="/admin/leaderboard" method="POST" enctype="multipart/form-data">
                <div class="form-group">
                    <label for="title">Name</label>
                    <input type="text" class="form-control" name="name" id="name" placeholder="Input Name" required
                        autocomplete="off">
                </div>

                <div class="form-group">
                    <label for="title">Nominal</label>
                    <input type="text" class="form-control" name="nominal" id="nominal" placeholder="Nominal" required
                        autocomplete="off">
                </div>

                <div class="form-group">
                    <label for="proker">Proker</label>
                    <select name="proker" id="proker" class="form-control" required>
                        <option value="" disabled selected>Select your Project</option>
                        {{#each projects}}
                        <option value={{title}}>{{title}}</option>
                        {{/each}}
                    </select>
                </div>

                <button class="btn btn-outline-success btn-lg" type="submit">Submit</button>
            </form>
        </div>
    </div>
</div>